<div class="row">
  <p>window.state = {{state}}</p>
  <p>window.store = {{state.store}}</p>
  <p>window.database = {{state.database}}</p>
</div>

{{#if documents.loadable}}
  <div class="row">
    {{#if documents.isLoading}}
      Loadingâ€¦
    {{else}}
      {{#each documents as |model|}}
        <div>{{model}}</div>
      {{/each}}
    {{/if}}
  </div>
{{else}}
  <div class="row" onclick={{action 'enable' target=documents}}>Load all documents</div>
{{/if}}
