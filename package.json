{
  "name": "ember-cli-documents",
  "version": "0.0.4",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon",
    "couchdb",
    "documents",
    "persistence",
    "rest"
  ],
  "license": "MIT",
  "author": "Arnis Vuskans <ampatspell@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ampatspell/ember-cli-documents.git"
  },
  "scripts": {
    "build": "ember build",
